{
  "name": "notepadaf",
  "version": "0.0.1",
  "description": "A notes app with PT/GL integration",
  "productName": "NotepadAF",
  "author": "AdamF <adaminthecape@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron",
    "build": "quasar build --mode electron --publish",
    "postinstall": "install-app-deps",
    "start": "npm install && electron ./app",
    "pack": "build --dir",
    "dist": "build"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.6.5",
    "node-html-markdown": "^1.3.0",
    "quasar": "^1.0.0",
    "vue-i18n": "^8.0.0",
    "vue-markdown": "^2.2.4",
    "vue-showdown": "^4.0.0",
    "vuedraggable": "^2.24.3"
  },
  "build": {
    "appId": "notepadaf",
    "category": "notes",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-qmarkdown": "^1.4.4",
    "devtron": "^1.4.0",
    "electron": "^9.4.4",
    "electron-builder": "^23.6.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^14.2.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^2.0.0",
    "prettier": "^2.5.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
